'use strict';

var app = angular.module('app.organizations');

app.controller('OrganizationsShowCtrl', ['$scope', '$state', 'Forum', function($scope, $state, Forum) {
    var defaultState = 'app.organizations.show.forums';
    if(!($state.current.name.indexOf(defaultState) > -1))
        $state.go(defaultState);
}]);